# Server Configuration (defaults shown)
# =============================================================================

# API Server
# PORT=11434
# HOST=0.0.0.0

# Admin UI
# ADMIN_PORT=8080
# ADMIN_HOST=0.0.0.0

# Debug logging
# DEBUG=false
