# Multi-Provider LLM Proxy Configuration
# Copy this file to .env and fill in your API keys

# =============================================================================
# API Keys (at least one required)
# =============================================================================

# Anthropic Claude
ANTHROPIC_API_KEY=

# OpenAI GPT
OPENAI_API_KEY=

# Google Gemini
GOOGLE_API_KEY=

# Perplexity
PERPLEXITY_API_KEY=

# Groq
GROQ_API_KEY=

# DeepSeek
DEEPSEEK_API_KEY=

# Mistral
MISTRAL_API_KEY=

# xAI Grok
XAI_API_KEY=

# OpenRouter (access 400+ models)
OPENROUTER_API_KEY=

